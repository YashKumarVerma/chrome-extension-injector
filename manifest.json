{
  "name": "chrome-extension-injector",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension plays around with github's input components",
  "homepage_url": "https://github-on-steroids.github.io/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo"
  },
  "permissions": ["clipboardRead", "clipboardWrite", "tabs"],
  "content_scripts": [
    {
      "matches": ["https://*.github.com/*"],
      "css": ["src/css/inject.css"]
    },
    {
      "matches": ["https://*.github.com/*"],
      "js": [
        "src/js/jquery.js",
        "src/js/emojis.js",
        "src/js/worker.js",
        "src/js/constants.js",
        "src/js/listeners.js",
        "src/js/inject.js"
      ]
    }
  ]
}
